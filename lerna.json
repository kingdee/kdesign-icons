{
  "packages": [
    "packages/*"
  ],
  "npmClient":"yarn",
  "useWorkspaces":true,
  "version": "independent",
  "command": {
    "publish": {
      "access":"public",
      "registry": "https://registry.npmjs.org/",
      "npmClientArgs": [
        "--registry=https://registry.npmjs.com/",
        "--scope=@kdcloudjs"
      ]
    },
    "version": {
      "ignore": ["**/*.md"]
    }
  }
}
